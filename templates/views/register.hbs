<html>
  <head>
    <title>Contact us</title>
    <link rel="stylesheet" type="text/css" href="./css/stylesr.css" />
    <link
      href="https://fonts.googleapis.com/css?family=Quicksand&display=swap"
      rel="stylesheet"
    />
    <meta
      name="viewport"
      content="width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0"
    />
  </head>
  <body>

    <nav class="navbar">
      <div class="brand-title">
        <img src="./images/College.png" alt="logoImage" class="logo-icon" />
        <div class="verBar"></div>
        <div class="company">
          <span>StreetLight</span>
          <p>Rajkiya Engineering College <br /> Sonbhadra U.P </p>
        </div>
      </div>
      <a href="#" class="toggle-button">
        <span class="bar"></span>
        <span class="bar"></span>
        <span class="bar"></span>

      </a>
      <div class="navbar-links">
        <ul>
          <li><a href="https://streetlightqrbased.onrender.com">Home</a></li>
          <li><a href="/register">Complaint</a></li>
          <li><a href="#">RTI</a></li>

          <li><a href="#">About</a></li>
          <li><a href="#">Contact Us</a></li>
        </ul>
      </div>
    </nav>

    {{! nav }}

    <div class="welcome">
      <h2>Welcome to our StreetLight <br /> Complaint page!</h2>
    </div>
    {{! welcome }}
    <div class="container">
      <div class="contact-box">
        <div class="left"></div>
        <form class="right" action="/register" method="POST" id="complaintForm">
          <h2>Report Us</h2>
          <input
            name="personname"
            type="text"
            class="field"
            placeholder="Your Name"
          />
          <input
            name="email"
            type="text"
            class="field"
            placeholder="Your Email"
          />
          <input name="Date" type="date" class="field" />
          <input
            name="lightId"
            type="text"
            class="field"
            placeholder="LightId"
          />
          <input
            name="areaPin"
            type="text"
            class="field"
            placeholder="area PIN code"
          />
          <textarea
            name="complaint"
            placeholder="Message"
            class="field"
          ></textarea>

          <input type="submit" class="btnRegister" value="Send" />

        </form>
      </div>
    </div>
    {{! footer }}
    <div class="footer-container">
      <footer class="footer">

        <div class="footer-top">

          <div class="comp-logo">
            <a class="logo-link" href="#">
              <img class="logo-svg" src="./images/College.png" />
              StreetLight
              <br />
              <p class="filler-text">Lighting Every Path!.</p>

            </a>
          </div>
          <div class="social">
            <a class="social-link" href="#">
              <img src="./images/instagram.svg" class="social-icon" />
            </a>
            <a class="social-link">
              <img src="./images/linkedin.svg" class="social-icon" />
            </a>
            <a class="social-link">
              <img src="./images/Microsoft.svg" class="social-icon" />
            </a>
            <a class="social-link">
              <img src="./images/twitter.svg" class="social-icon" />
            </a>
          </div>
        </div>

        <div class="footer-grid">
          <!-- column 1 -->
          <div class="footer-grid-column">
            <div class="footer-grid-heading">
              Products
            </div>
            <ul class="footer-links-list">
              <li>
                <a href="#overview" class="footer-link">Overview</a>
              </li>
              <li>
                <a href="#overview" class="footer-link">Solutions</a>
              </li>
              <li>
                <a href="#overview" class="footer-link">Pricing</a>
              </li>
              <li>
